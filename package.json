{
  "name": "valid-assessment",
  "version": "0.9.0",
  "description": "VALID Decision-Making Style Assessment Tool",
  "main": "index.js",
  "scripts": {
    "start": "http-server -p 8080",
    "test": "jest",
    "lint": "eslint js/",
    "format": "prettier --write .",
    "db:reset": "supabase db reset",
    "db:migrate": "supabase db migrate",
    "deploy:staging": "vercel --env VALID_ENV=staging",
    "deploy:prod": "vercel --prod --env VALID_ENV=production",
    "build": "node build.js"
  },
  "dependencies": {
    "@supabase/auth-js": "^2.70.0",
    "@supabase/functions-js": "^2.4.4",
    "@supabase/postgrest-js": "^1.19.4",
    "@supabase/realtime-js": "^2.11.9",
    "@supabase/storage-js": "^2.7.3",
    "@supabase/supabase-js": "2.39.0",
    "emailjs-com": "3.2.0",
    "esbuild": "^0.25.5"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "http-server": "14.1.1",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "supabase": "1.123.4"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/valid-assessment.git"
